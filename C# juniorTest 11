using System;

internal class Program
{
    static void Main(string[] args)
    {
        int startNumber = 5;
        int stepProgression = 7;
        int maxCounterProgression = 100;

        for (int i = startNumber; i < maxCounterProgression; i += stepProgression)
        {
            Console.Write(" " + i);
        }
        Console.Write("\n");
    }
}
