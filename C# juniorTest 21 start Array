using System;

internal class Program
{
    static void Main(string[] args)
    {
        int[,] array = new int[3, 3];
        Random random = new Random();
        int sumSecondString = 0;
        int multiplicationFirstColumn = 1;
        int maxDataArray = 9;
        int minDataArray = 0;
        int numberString = 2;
        int numberColumn = 1;

        for (int i = 0; i < array.GetLength(0); i++)
        {
            for (int j = 0; j < array.GetLength(1); j++)
            {
                array[i, j] = random.Next(minDataArray, maxDataArray);
                Console.Write(array[i, j] + " ");
            }
            Console.WriteLine();
        }

        for (int i = 0; i < array.GetLength(0); i++)
        {
            multiplicationFirstColumn *= array[i, (numberColumn - 1)];
        }

        for (int i = 0; i < array.GetLength(1); i++)
        {
            sumSecondString += array[(numberString - 1), i];
        }
        Console.WriteLine("\n Сумма 2й строки = " + sumSecondString);
        Console.WriteLine(" Произведение 1го столбца = " + multiplicationFirstColumn + "\n");
    }
}
