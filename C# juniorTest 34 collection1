using System;
using System.Collections.Generic;


internal class Program
{
    static void Main(string[] args)
    {
        string amountBig = "очень много";
        string amountMiddle = "много";
        string amountSmall = "немного";

        Dictionary<string, string> populations = new Dictionary<string, string>()
        {
            {"Москва", amountBig},
            {"СПб", amountBig},
            {"Владивосток", amountMiddle},
            {"Новосибирск", amountMiddle},
            {"Киров", amountSmall},
            {"Вологда", amountSmall}
        };

        Console.Write("Впиши город, узнаешь плотность населения: ");
        string cityName = Console.ReadLine();

        Console.WriteLine(GetPopulationCount(cityName, populations));
    }

    static string GetPopulationCount(string cityName, Dictionary<string, string> lists)
    {
        string dataMessage = "\n Пусто.\n";

        if (lists.ContainsKey(cityName))
        {
            dataMessage = "\n Город: " + cityName + ", население: " + lists[cityName] + ".\n";
        }

        return dataMessage;
    }
}
